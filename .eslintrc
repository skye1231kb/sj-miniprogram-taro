{
  "extends": ["taro"],
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "Taro" }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx"] }]
  },
  "parser": "babel-eslint",
  "parserOptions": {

    "ecmaFeatures": {

      "jsx": true

    },

    "useJSXTextNode": true,

    "project": "./jsconfig.json"

  }
}
